// This program include math library function

#include "stdafx.h"
#include <iostream>
#include <cmath>
using namespace std;

void main()
{
	cout << abs(9.5) << endl;
	cout << floor(9.5) << endl;
	cout << floor(-9.5) << endl;
	cout << ceil(8.5) << endl;
	cout << ceil(-8.5) << endl;
	cout << sqrt(pow(6, 4)) << endl;
}
