// This program include array

#include "stdafx.h"
#include <iostream>
#define SIZE 10
using namespace std;


void main()
{
	int numbers[SIZE] = { 5,10,15,20,25,30,35,40,45,50 };
	int total = 0, average;

	for (int i = 0; i <= 9; i++)
		total += numbers[i];

	average = total / SIZE;

	cout << "The average value of elements in array numbers" << " is:" << average << endl;
}
