//This program lets user enter 5 subject marks for 30 students

#include <iostream>
using namespace std;

int main()
{
	int marks, total = 0, average, lowest, highest;
		int studMarks[6][5];
		for (int i = 0; i < 6; i++)
		{
			for (int j = 0; j < 5; j++)
				cin >> marks;
			studMarks[6][5] = marks;
		}

		for (int i = 0; i < 6; i++)
		{
			for (int j = 0; j < 5; j++)
				total += studMarks[6][5];
		}
		cout << total;

		average = total / 30;
		cout << average;

		lowest = studMarks[0][0];

		for (int i = 0; i < 6; i++)
		{
			for (int j = 0; j < 5; j++)
				if (lowest < studMarks[i][i])
					lowest = studMarks[i][i];
				else
					highest = studMarks[i][i];
		}
		cout << lowest;
		cout << highest;

}
